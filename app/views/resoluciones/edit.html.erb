<h1>Editing resolucion</h1>

<% form_for(@resolucion) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :numero %><br />
    <%= f.text_field :numero %>
  </p>
  <p>
    <%= f.label :solicitud_id %><br />
    <%= f.text_field :solicitud_id %>
  </p>
  <p>
    <%= f.label :usuario_id %><br />
    <%= f.text_field :usuario_id %>
  </p>
  <p>
    <%= f.label :institucion_id %><br />
    <%= f.text_field :institucion_id %>
  </p>
  <p>
    <%= f.label :descripcion %><br />
    <%= f.text_area :descripcion %>
  </p>
  <p>
    <%= f.label :tiporesolucion_id %><br />
    <%= f.text_field :tiporesolucion_id %>
  </p>
  <p>
    <%= f.label :razonresolucion_id %><br />
    <%= f.text_field :razonresolucion_id %>
  </p>
  <p>
    <%= f.label :nueva_fecha %><br />
    <%= f.date_select :nueva_fecha %>
  </p>
  <p>
    <%= f.submit 'Update' %>
  </p>
<% end %>

<%= link_to 'Show', @resolucion %> |
<%= link_to 'Back', resoluciones_path %>