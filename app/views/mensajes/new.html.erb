<% content_for :head do %>
  <%= javascript_include_tag 'mensajes', 'jquery.ui.datepicker-es.js' %>
<% end %>

<div class="mantform">
  <h2>Nuevo <%= Mensaje.human_name %></h2>
  
  <% semantic_form_for @mensaje, :url => institucion_mensajes_path(@institucion) do |form| %>
    <%= form.semantic_errors %>
    <%= render :partial => 'form', :locals => {:form => form} %>    
    <%= buttons_create(recibidos_institucion_mensajes_path(@institucion)) %>
  <% end %>
</div>
