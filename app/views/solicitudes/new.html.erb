<% content_for :head do %>
  <%= javascript_include_tag 'solicitudes', 'jquery.ui.datepicker-es.js' %>
<% end %>

<% @forview = "NEW" %>
<%= render :partial => "breadcrums" %>
<div class="mantform">
  <h2>Nueva <%= Solicitud.human_name %></h2>
  
  <% semantic_form_for @solicitud do |form| %>
    <%= form.semantic_errors %>
    <%= render :partial => 'form', :locals => {:form => form} %>    
    <%= buttons_create(solicitudes_path) %>
  <% end %>
</div>
