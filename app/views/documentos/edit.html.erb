<% content_for :head do %>
  <%= javascript_include_tag 'combobox.js', 'documentos.js', 'jquery.ui.datepicker-es.js' %>
<% end %>

<div class="mantform">
  <h2>Editar <%= Documento.human_name %></h2>
  
  <% semantic_form_for @documento do |form| %>
    <%= form.semantic_errors %>
    <%= render :partial => 'form', :locals => {:form => form} %>     
    <%= buttons_update(documentos_path) %>
  <% end %>
  
</div>

