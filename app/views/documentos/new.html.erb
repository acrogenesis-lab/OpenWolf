<% content_for :head do %>
  <%= javascript_include_tag 'combobox.js', 'documentos.js', 'jquery.ui.datepicker-es.js' %>
<% end %>

<div class="mantform">
  <h2>Nuevo <%= Documento.human_name %></h2>
  
  <% semantic_form_for @documento, :url => documentos_path(@institucion) do |form| %>
    <%= form.semantic_errors %>
    <%= render :partial => 'form', :locals => {:form => form} %>    
    <%= buttons_create(documentos_path(@institucion)) %>
  <% end %>
</div>
