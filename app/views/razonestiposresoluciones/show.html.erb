<div class="show">
  <h2><%= Razontiporesolucion.human_name %></h2>
  
  <div id="options">
    <%= button_new(new_razontiporesolucion_path) %>
    <%= button_edit(edit_razontiporesolucion_path(@razontiporesolucion)) %>
    <%= button_delete(razontiporesolucion_path(@razontiporesolucion)) %>
    <%= button_cancel(razonestiposresoluciones_path) %>
    <br />
  </div>
  
  <div class="box">
    <table>
      <colgroup class="show-table-header"></colgroup>
      <colgroup class="show-table-data"></colgroup>
      <tr>
        <th><%= label :nombre, Razontiporesolucion.human_attribute_name(:nombre) %></th>
        <td><%= @razontiporesolucion.nombre %></td>
      </tr>
      <tr>
        <th><%= label :tiporesolucion_id, Razontiporesolucion.human_attribute_name(:tiporesolucion_id) %></th>
        <td><%= @razontiporesolucion.tiporesolucion.nombre %></td>
      </tr>
      <tr>
        <th><%= label :informacion_publica, Razontiporesolucion.human_attribute_name(:informacion_publica) %></th>
        <td><%= @razontiporesolucion.informacion_publica %></td>
      </tr>
    </table>     
  </div>
  
</div>
